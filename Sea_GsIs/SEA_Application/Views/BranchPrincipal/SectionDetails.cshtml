@model List<SEA_Application.Models.AspNetBranchClass_Sections>

<h2>Section Details</h2>

<div class="box box-primary">
    <div class="box-body">
        <div class="form-horizontal">
                @foreach (var item in Model)
                {
                    <div class="form-group">
                        @Html.LabelFor(m => item.SectionId, new { @class = "control-label col-md-2 " })
                        <div class="col-md-6">
                            @Html.EditorFor(m => item.SectionId, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>

                    </div>

                    <div class="form-group">
                        @Html.LabelFor(m => item.AspNetSection, new { @class = "control-label col-md-2 " })
                        <div class="col-md-6">
                            @Html.EditorFor(m => item.AspNetSection.Name, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(m => item.AspNetBranch_Class.AspNetClass, new { @class = "control-label col-md-2 " })
                        <div class="col-md-6">
                            @Html.EditorFor(m => item.AspNetBranch_Class.AspNetClass.Name, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                    </div>

                    <div class="form-group">
                        @Html.LabelFor(m => item.AspNetBranch_Class.AspNetBranch, new { @class = "control-label col-md-2 " })
                        <div class="col-md-6">
                            @Html.EditorFor(m => item.AspNetBranch_Class.AspNetBranch.Name, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                        </div>
                    </div>

                }
        </div>
    </div>
</div>