
@{
    ViewBag.Title = "Index";


    Layout = "~/Views/Shared/_StudentDashboardLayout.cshtml";
@*<a href="~/Views/Shared/_ParentDashboardLayout.cshtml">~/Views/Shared/_ParentDashboardLayout.cshtml</a>*@
}

<div class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="pageHeading">
                    <div class="row">
                        <div class="col-sm-12">
                            <h1 class="float-left">Subjects List</h1>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card">

                    <div class="p-5">

                        <div class="row" id="Subjectlist">
                     </div>

                </div>
            </div>

        </div>
    </div>
</div>
    </div>

<script>

    $(document).ready(function () {

     
        
        $("#BreadCrumbs").append(' <li><a href="/Student/Index"> Home </a> <span>&gt;</span></li>');
        $("#BreadCrumbs").append(' <li class="active"><a id="BreadCrumps1" href="#">Subjects</a></li>');
        $.ajax({
            type: "POST",
            url: "/StudentCourses/AllSubjectsOfStudent",
            data: {},
            success: function (data) {

                $.each(data, function (key, value1) {

                    $("#Subjectlist").append(' <div class="col-md-6 col-lg-3"> <a href="/StudentCourses/SubjectTopics?id=' + value1.Id + '" class="subject-list-box"> ' + value1.Name + ' <br />     </a></div>');

                })

            }
        })
    })

</script>